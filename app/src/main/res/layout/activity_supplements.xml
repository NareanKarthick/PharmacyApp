                tools:ignore="NotSibling">

                <ImageView
                    android:id="@+id/image14"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginStart="14dp"
                    android:src="@drawable/fc"
                    app:layout_constraintBottom_toBottomOf="@+id/textView12"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@id/layoutayurvedic"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/white_back"
                    android:backgroundTint="@color/i_blue"
                    android:paddingStart="8dp"
                    android:paddingTop="2dp"
                    android:paddingEnd="8dp"
                    android:paddingBottom="2dp"
                    android:text="Fitness and Supplements"
                    android:textColor="@color/white"
                    android:textSize="22dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.321"
                    app:layout_constraintStart_toEndOf="@id/image14"
                    app:layout_constraintTop_toTopOf="parent" />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layoutelder"
                android:layout_width="402dp"
                android:layout_height="130dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/header_background"
                android:padding="20dp"
                app:layout_constraintTop_toBottomOf="@id/layoutfitness"
                tools:layout_editor_absoluteX="4dp"
                tools:ignore="NotSibling">

                <ImageView
                    android:id="@+id/image15"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginStart="14dp"
                    android:src="@drawable/ec"
                    app:layout_constraintBottom_toBottomOf="@+id/textView12"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@id/layoutfitness"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/white_back"
                    android:backgroundTint="@color/i_blue"
                    android:paddingStart="8dp"
                    android:paddingTop="2dp"
                    android:paddingEnd="8dp"
                    android:paddingBottom="2dp"
                    android:text="Elderly Care"
                    android:textColor="@color/white"
                    android:textSize="22dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.321"
                    app:layout_constraintStart_toEndOf="@id/image15"
                    app:layout_constraintTop_toTopOf="parent" />


            </androidx.constraintlayout.widget.ConstraintLayout>


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layoutmothercare"
                android:layout_width="402dp"
                android:layout_height="130dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/header_background"
                android:padding="20dp"
                app:layout_constraintTop_toBottomOf="@id/layoutelder"
                tools:layout_editor_absoluteX="4dp"
                tools:ignore="NotSibling">

                <ImageView
                    android:id="@+id/image16"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginStart="14dp"
                    android:src="@drawable/mc"
                    app:layout_constraintBottom_toBottomOf="@+id/textView12"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@id/layoutelder"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/white_back"
                    android:backgroundTint="@color/i_blue"
                    android:paddingStart="8dp"
                    android:paddingTop="2dp"
                    android:paddingEnd="8dp"
                    android:paddingBottom="2dp"
                    android:text="Mother and Baby Care"
                    android:textColor="@color/white"
                    android:textSize="22dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.321"
                    app:layout_constraintStart_toEndOf="@id/image16"
                    app:layout_constraintTop_toTopOf="parent" />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layoutendcare"
                android:layout_width="402dp"
                android:layout_height="130dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/header_background"
                android:padding="20dp"
                app:layout_constraintTop_toBottomOf="@id/layoutmothercare"
                tools:layout_editor_absoluteX="4dp"
                tools:ignore="NotSibling">


            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>



</androidx.constraintlayout.widget.ConstraintLayout>